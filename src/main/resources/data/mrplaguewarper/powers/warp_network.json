{
    "type": "origins:multiple",
	"name": "Warp Network",
	"description": "Press your secondary ability key to toggle warp network access. When access is enabled, enter interdimensional sight and press hotbar keys 1-6 to set warp points. Exit interdimensional influence and press hotbar keys 1-6 to teleport to your warp points. Warp points have a range of 900 blocks, and erase themselves when you close your client.",
    "toggle": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "mrplaguewarper:warp_network_bar",
                "comparison": "==",
                "compare_to": 1
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "mrplaguewarper:indexed_sound",
                        "index": 2
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "mrplaguewarper:warp_network_bar",
                        "change": 0,
                        "operation": "set"
                    }
                ]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "mrplaguewarper:indexed_sound",
                        "index": 1
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "mrplaguewarper:warp_network_bar",
                        "change": 1,
                        "operation": "set"
                    }
                ]
            }
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.origins.secondary_active"
        }
    },
    "bar": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    }
}